<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="questionLabel" text="Question">
         <VBox.margin>
            <Insets bottom="30.0" top="30.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="answerLabel" text="Answer" textFill="#07a431" visible="false">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <font>
            <Font size="22.0" />
         </font>
      </Label>
   </children>
</VBox>
